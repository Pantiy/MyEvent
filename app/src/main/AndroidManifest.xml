<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.pantiy.myevent">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.EventListPagerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.EventPagerActivity"
            android:parentActivityName=".activity.EventListPagerActivity"/>
        <!--<activity android:name=".DatePickerActivity"/>-->
        <!--<activity android:name=".TimePickerActivity"/>-->
    </application>

</manifest>